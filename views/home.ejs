<main id="home">
   <section>
    <% if(locals.user){%>
        <div id="feed-post">
            <section>
                <h4>Posts</h4>
                <form action="/posts/create" method="post" id="new-post">
                    <textarea name="content"  cols="30" rows="4" placeholder="write here......"></textarea>
                    <button type="submit">post</button>
                </form>
            </section>
    
        </div>
    <%}%>
</section>
<section>
    <div id="post-contentainer">

        <% for(post of  posts) { %>

            <%-include('_post')-%>

        <% } %> 
    
</section>

<section id="all_usersList">
        <%for(u of allUsers){%>
            <li><a href="users/profile/<%=u._id%>"><%=u.name%></a></li>
        <%}%>
</section>
</main>